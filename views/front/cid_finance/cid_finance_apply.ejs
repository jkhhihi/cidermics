	document.applyform.app_phone2.focus();
	  return;
	}
	if(document.applyform.app_phone3.value==""){
	 alert("휴대전화 번호를 입력해 주세요.");
	document.applyform.app_phone3.focus();
	  return;
	}
	if(document.applyform.app_email1.value==""){
	 alert("이메일 아이디를 입력해 주세요.");
	document.applyform.app_email1.focus();
	  return;
	}
	if(document.applyform.app_email2.value==""){
	 alert("이메일를 입력해 주세요.");
	document.applyform.app_email2.focus();
	  return;
	}
	for (i=0; i<document.applyform.app_email1.value.length; i++)
        {
         var ch = document.applyform.app_email1.value.charAt(i);//문자를 반환(정수형), 범위 검사 가능
	if ( ( ch < "a" || ch > "z") && (ch < "A" || ch > "Z") && (ch < "0" || ch > "9" ) )
    	{
            alert("이메일은 영문 소문자로만 입력 가능 합니다!");
            document.applyform.app_email1.focus();
            return;
         }
		}
	
	
	for (i=0; i<document.applyform.app_phone2.value.length; i++)
     {
     	 var ch = document.applyform.app_phone2.value.charAt(i);//문자를 반환(정수형), 범위 검사 가능
		if ( ch < "0" || ch > "9" )
    	 {
            alert("휴대폰 번호는 숫자로만 입력 가능 합니다!");
            document.applyform.app_phone2.focus();
            return;
         }
      }
	 
	 	document.applyform.submit();
}
